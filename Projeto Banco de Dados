( 
); 

CREATE TABLE Técnico 
( 
 Função INT,  
); 

CREATE TABLE Consulta 
( 
 Tipo diagnóstico INT,  
 Descrição diagnóstico INT,  
 Data atendimento INT,  
 Horário atendimento INT,  
 Sala INT,  
); 

CREATE TABLE Exame 
( 
 Tipo resultado INT,  
 Tipo de exame INT,  
 Descrição diagnóstico INT,  
 Data atendimento INT,  
 Descritivo resultado INT,  
 Resultado INT,  
 Horário atendimento INT,  
); 

CREATE TABLE Laboratório 
( 
 Nome INT,  
 Telefone INT,  
 Endereço INT,  
 Exames cobertos INT,  
 Data Expiração Contrato INT,  
 Valor cobrado INT,  
); 

CREATE TABLE Internação 
( 
 Motivo INT,  
 Exame.Resultado INT,  
 Leito.Sala INT,  
 Solicitação INT,  
); 

CREATE TABLE Cirurgia 
( 
 Sala INT,  
 Especialidade INT,  
 Andar INT,  
); 

CREATE TABLE Leito 
( 
 Andar INT,  
 ID Leito INT PRIMARY KEY,  
 Sala INT,  
); 

