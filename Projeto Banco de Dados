CREATE TABLE Ala 
( 
 Tipo INT,  
 Nome INT,  
); 

CREATE TABLE Pessoa 
( 
 Nome INT,  
 Telefone INT,  
 Gênero INT,  
 Data nascimento INT,  
 CPF INT PRIMARY KEY,  
 Endereço INT,  
); 

CREATE TABLE Paciente 
( 
 ID INT PRIMARY KEY,  
 Numero Carteirinha SUS INT,  
); 

CREATE TABLE Funcionário 
( 
 Data contratação INT,  
); 

CREATE TABLE Médico 
( 
 Especialidade INT,  
 CRM INT PRIMARY KEY,  
); 

